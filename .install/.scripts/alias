#!/bin/bash
alias ls='ls --color=auto'
alias ll='ls -l'
alias la='ls -A'
alias l='ls -CF'
alias g='git'
alias grep='grep --color=auto'  
alias fgrep='fgrep --color=auto'    
alias egrep='egrep --color=auto'    
alias c='clear' 
alias v='vim'   
alias h='history'   
alias q='exit'   
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'